<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="SFA.DAS.Data" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WorkerRole name="SFA.DAS.Data.AccountRegistration" vmsize="Small">
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
    </ConfigurationSettings>
  </WorkerRole>
  <WorkerRole name="SFA.DAS.Data.AccountBalance" vmsize="Small">
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" />
      <Setting name="AccountApi.ApiBaseUrl"/>
      <Setting name="AccountApi.ClientSecret"/>
      <Setting name="AccountApi.ClientId"/>
      <Setting name="AccountApi.IdentifierUri"/>
      <Setting name="AccountApi.Tenant"/>
      <Setting name="StagingConnectionString"/>
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>